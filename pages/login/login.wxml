<!--logs.wxml-->
<view class="page">
  <form bindsubmit='submitForm'>
    <view class="container">
      <view class='container-info'>
        <view class="man-form-info">
          <view class='username'>
            <text>用户名</text>
            <input placeholder='请输入用户名' type='text' name='username'></input>
          </view>
          <view class='password'>
            <text>密码</text>
            <input placeholder='请输入密码' type='password' name='password'></input>
          </view>
        </view>
      </view>

      <view class='submit-form-info'>
        <button form-type='submit'>登录</button>
      </view>

      <view class='register'>
        <button bindtap='gotoregister'>新用户注册</button>
      </view>


    </view>
  </form>
</view>